/**index.wxss**/

page {
  height: 100%;
  box-sizing: border-box;
}

.container {
  background: #f9f9f9;
  height: 100%;
  box-sizing: border-box;
  overflow: auto;
  padding: 0 20px;
}

.list {
  height: auto;
  overflow: hidden;
  line-height: 45px;
  color: #494949;
  font-size: 16px;
  font-weight: bold;
}

.list text {
  float: right;
  color: #666;
  font-weight: 500;
  font-size: 16px;
}

.tips {
  color: #666;
  font-size: 14px;
  padding: 20px;
  box-sizing: border-box;
  text-align: center;
}

.tips navigator {
  display: inline-block;
  color: #00f;
  font-weight: bold;
  font-size: 16px;
}